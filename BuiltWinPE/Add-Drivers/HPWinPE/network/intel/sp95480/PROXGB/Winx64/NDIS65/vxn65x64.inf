[Version]
Signature   = "$Windows NT$"
Class       = Net
ClassGUID   = {4d36e972-e325-11ce-bfc1-08002be10318}
Provider    = %Intel%
CatalogFile = vxn65x64.cat
DriverVer = 09/25/2018,2.1.133.0

[Manufacturer]
%Intel%     = Intel, NTamd64.10.0

[Intel]

[Intel.NTamd64.10.0]
; DisplayName                       Section         DeviceID
; -----------                       -------         --------
%E10EDVF.DeviceDesc%               = VF8086,        PCI\VEN_8086&DEV_10ED
%E1515VF.DeviceDesc%               = VF8086,        PCI\VEN_8086&DEV_1515
%E10EDVF.DeviceDesc%               = HV8086_NO_RSC, PCI\VEN_8086&DEV_152E 
%E1515VF.DeviceDesc%               = HV8086_NO_RSC, PCI\VEN_8086&DEV_1530 

;-------------------------------------------------------------------------------
; Intel Virtual Function
;
[VF8086]
Characteristics    = 0x84 ; NCF_HAS_UI | NCF_PHYSICAL
BusType            = 5 ; PCI
DelReg             = Cleanup.DelReg
AddReg             = Interfaces_vf.reg, Default.reg, VLAN.reg, Uninstall.reg
CopyFiles          = CopyFiles, MessageDll.CopyFiles, Uninstall.CopyFiles
*IfType            = 6  ; IF_TYPE_ETHERNET_CSMACD
*MediaType         = 0  ; NdisMedium802_3
*PhysicalMediaType = 0  ; NdisPhysicalMediumUnspecified

[VF8086.Services]
AddService = %ServiceName%, 2, Service, EventLog

[VF8086.HW]
Include            = machine.inf
Needs              = PciIoSpaceNotRequired
AddReg             = MSI.reg

[VF8086.CoInstallers]
AddReg             = CoInstaller_AddReg, CoInK.reg
CopyFiles          = CoInstallerCopyFiles

;-------------------------------------------------------------------------------
; Intel Virtual Function - Windows Hypervisor with RSC
;
[HV8086_RSC]
Characteristics    = 0x84 ; NCF_HAS_UI | NCF_PHYSICAL
BusType            = 5 ; PCI
DelReg             = Cleanup.DelReg
AddReg             = Interfaces_hyperv.reg, Default.reg, RSC.reg, Uninstall.reg
CopyFiles          = CopyFiles, MessageDll.CopyFiles, Uninstall.CopyFiles
*IfType            = 6  ; IF_TYPE_ETHERNET_CSMACD
*MediaType         = 0  ; NdisMedium802_3
*PhysicalMediaType = 0  ; NdisPhysicalMediumUnspecified

[HV8086_RSC.Services]
AddService = %ServiceName%, 2, Service, EventLog

[HV8086_RSC.HW]
Include            = machine.inf
Needs              = PciIoSpaceNotRequired
AddReg             = MSI.reg

[HV8086_RSC.CoInstallers]
AddReg             = CoInstaller_AddReg, CoInK.reg
CopyFiles          = CoInstallerCopyFiles

;-------------------------------------------------------------------------------
; Intel Virtual Function - Windows Hypervisor without RSC
;
[HV8086_NO_RSC]
Characteristics    = 0x84 ; NCF_HAS_UI | NCF_PHYSICAL
BusType            = 5 ; PCI
DelReg             = Cleanup.DelReg
AddReg             = Interfaces_hyperv.reg, Default.reg, Uninstall.reg
CopyFiles          = CopyFiles, MessageDll.CopyFiles, Uninstall.CopyFiles
*IfType            = 6  ; IF_TYPE_ETHERNET_CSMACD
*MediaType         = 0  ; NdisMedium802_3
*PhysicalMediaType = 0  ; NdisPhysicalMediumUnspecified

[HV8086_NO_RSC.Services]
AddService = %ServiceName%, 2, Service, EventLog

[HV8086_NO_RSC.HW]
Include            = machine.inf
Needs              = PciIoSpaceNotRequired
AddReg             = MSI.reg

[HV8086_NO_RSC.CoInstallers]
AddReg             = CoInstaller_AddReg, CoInK.reg
CopyFiles          = CoInstallerCopyFiles

;-------------------------------------------------------------------------------
; Registry Settings
;-------------------------------------------------------------------------------

[Interfaces_vf.reg]
HKR, Ndi\Interfaces,                            UpperRange,             0, "ndis5"
HKR, Ndi\Interfaces,                            LowerRange,             0, "ethernet"
HKR, Ndi,                                       Service,                0, %ServiceName%

[Interfaces_hyperv.reg]
HKR, Ndi\Interfaces,                            UpperRange,             0, "ndisvf"
HKR, Ndi\Interfaces,                            LowerRange,             0, "iovvf"
HKR, Ndi,                                       Service,                0, %ServiceName%

[Default.reg]
; *TransmitBuffers
HKR, Ndi\params\*TransmitBuffers,               ParamDesc,              0, %TransmitBuffers%
HKR, Ndi\params\*TransmitBuffers,               default,                0, "512"
HKR, Ndi\params\*TransmitBuffers,               min,                    0, "128"
HKR, Ndi\params\*TransmitBuffers,               max,                    0, "16384"
HKR, Ndi\params\*TransmitBuffers,               step,                   0, "64"
HKR, Ndi\params\*TransmitBuffers,               Base,                   0, "10"
HKR, Ndi\params\*TransmitBuffers,               type,                   0, "int"

; *ReceiveBuffers
HKR, Ndi\params\*ReceiveBuffers,                ParamDesc,              0, %ReceiveBuffers%
HKR, Ndi\params\*ReceiveBuffers,                default,                0, "512"
HKR, Ndi\params\*ReceiveBuffers,                min,                    0, "128"
HKR, Ndi\params\*ReceiveBuffers,                max,                    0, "4096"
HKR, Ndi\params\*ReceiveBuffers,                step,                   0, "64"
HKR, Ndi\params\*ReceiveBuffers,                Base,                   0, "10"
HKR, Ndi\params\*ReceiveBuffers,                type,                   0, "int"

; Set MinHardwareOwnedPacketCount to 32
HKR,,                                           MinHardwareOwnedPacketCount,   0, "32"

; *TCPChecksumOffloadIPv4
