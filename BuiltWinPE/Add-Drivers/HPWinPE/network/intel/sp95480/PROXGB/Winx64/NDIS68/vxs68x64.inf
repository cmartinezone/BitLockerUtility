[Version]
Signature   = "$Windows NT$"
Class       = Net
ClassGUID   = {4d36e972-e325-11ce-bfc1-08002be10318}
Provider    = %Intel%
CatalogFile = vxs68x64.cat
DriverVer = 10/03/2018,2.1.138.0

[Manufacturer]
%Intel%     = Intel, NTamd64.10.0

[Intel]

[Intel.NTamd64.10.0]
; DisplayName                       Section         DeviceID
; -----------                       -------         --------
%E1564VF.DeviceDesc%               = VF8086,        PCI\VEN_8086&DEV_1565
%E15A8VF.DeviceDesc%               = VF8086,        PCI\VEN_8086&DEV_15A8
%E15C5VF.DeviceDesc%               = VF8086,        PCI\VEN_8086&DEV_15C5
%E1564VF.DeviceDesc%               = HV8086_NO_RSC, PCI\VEN_8086&DEV_1564
%E15A8VF.DeviceDesc%               = HV8086_NO_RSC, PCI\VEN_8086&DEV_15A9
%E15C5VF.DeviceDesc%               = HV8086_NO_RSC, PCI\VEN_8086&DEV_15B4

;-------------------------------------------------------------------------------
; Intel Virtual Function
;
[VF8086]
Characteristics    = 0x84 ; NCF_HAS_UI | NCF_PHYSICAL
BusType            = 5 ; PCI
DelReg             = Cleanup.DelReg
AddReg             = Interfaces_vf.reg, Default.reg, VLAN.reg, Uninstall.reg
CopyFiles          = CopyFiles, MessageDll.CopyFiles, Uninstall.CopyFiles
AddReg             = CoInK.reg
*IfType            = 6  ; IF_TYPE_ETHERNET_CSMACD
*MediaType         = 0  ; NdisMedium802_3
*PhysicalMediaType = 0  ; NdisPhysicalMediumUnspecified

[VF8086.Services]
AddService = %ServiceName%, 2, Service, EventLog

[VF8086.HW]
Include            = machine.inf
Needs              = PciIoSpaceNotRequired
AddReg             = MSI.reg

;-------------------------------------------------------------------------------
; Intel Virtual Function - Windows Hypervisor with RSC
;
[HV8086_RSC]
Characteristics    = 0x84 ; NCF_HAS_UI | NCF_PHYSICAL
BusType            = 5 ; PCI
DelReg             = Cleanup.DelReg
AddReg             = Interfaces_hyperv.reg, Default.reg, RSC.reg, Uninstall.reg
CopyFiles          = CopyFiles, MessageDll.CopyFiles, Uninstall.CopyFiles
AddReg             = CoInK.reg
*IfType            = 6  ; IF_TYPE_ETHERNET_CSMACD
*MediaType         = 0  ; NdisMedium802_3
*PhysicalMediaType = 0  ; NdisPhysicalMediumUnspecified

[HV8086_RSC.Services]
AddService = %ServiceName%, 2, Service, EventLog

[HV8086_RSC.HW]
Include            = machine.inf
Needs              = PciIoSpaceNotRequired
AddReg             = MSI.reg

;-------------------------------------------------------------------------------
; Intel Virtual Function - Windows Hypervisor without RSC
;
[HV8086_NO_RSC]
Characteristics    = 0x84 ; NCF_HAS_UI | NCF_PHYSICAL
BusType            = 5 ; PCI
DelReg             = Cleanup.DelReg
AddReg             = Interfaces_hyperv.reg, Default.reg, Uninstall.reg
CopyFiles          = CopyFiles, MessageDll.CopyFiles, Uninstall.CopyFiles
AddReg             = CoInK.reg
*IfType            = 6  ; IF_TYPE_ETHERNET_CSMACD
*MediaType         = 0  ; NdisMedium802_3
*PhysicalMediaType = 0  ; NdisPhysicalMediumUnspecified

[HV8086_NO_RSC.Services]
AddService = %ServiceName%, 2, Service, EventLog

[HV8086_NO_RSC.HW]
Include            = machine.inf
Needs              = PciIoSpaceNotRequired
AddReg             = MSI.reg

